{
  "name": "number-acidizer-backend",
  "version": "1.0.0",
  "description": "ACID-compliant distributed counter backend",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "dev": "ts-node-dev --respawn --transpile-only src/local/server.ts",
    "test": "jest",
    "test:unit": "jest --testRegex .spec.test.ts$",
    "test:integration": "jest --testRegex .int.test.ts$",
    "test:e2e": "jest --testRegex .e2e.test.ts$",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean"
  },
  "dependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.454.0",
    "@aws-sdk/client-dynamodb": "^3.454.0",
    "@aws-sdk/lib-dynamodb": "^3.454.0",
    "aws-lambda": "^1.0.7",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "uuid": "^9.0.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.125",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
